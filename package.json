    "koa-compose": "^4.1.0",
    "koa-ejs": "^4.1.2",
    "koa-json": "^2.0.2",
    "koa-route": "^3.2.0",
    "koa-static": "^5.0.0",
    "mysql2": "^1.6.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6"
  }
